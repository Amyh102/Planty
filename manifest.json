{
    "name": "Planty",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Planty",
        "default_icon": "logo128.png"
    },
    "icons": { "16": "logo16.png",
           "48": "logo48.png",
          "128": "logo128.png" }

}
